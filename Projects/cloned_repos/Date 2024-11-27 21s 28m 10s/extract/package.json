    "@remix-run/cloudflare": "^2.10.0",
    "@remix-run/cloudflare-pages": "^2.10.0",
    "@remix-run/react": "^2.10.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "isbot": "^5.1.10",
    "lucide-react": "^0.397.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
